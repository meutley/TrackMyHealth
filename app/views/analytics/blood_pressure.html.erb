<div class="container">
    <div class="row">
        <div class="col-sm-12">
            <h1>Blood Pressure Trends (10 Most Recent)</h1>
            <%= render partial: 'analytics/no_data' %>
            <div class="analytics-data">
                <canvas id="pressure-trends" width="600" height="200"></canvas>
            </div>
        </div>
    </div>
</div>

<script type="text/javascript">
    $(document).on('turbolinks:load', () => {
        Analytics.BloodPressure.init();
    });
</script>